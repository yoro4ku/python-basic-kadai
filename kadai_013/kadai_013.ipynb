      "source": [
        "def calculate_tax(amount, tax):\n",
        "    tax_rate = float(tax.strip('%')) / 100\n",
        "    amount_tax = amount * tax_rate\n",
        "    return amount + amount_tax\n",
        "\n",
        "\n",
        "print(calculate_tax(110, \"10%\"))\n"
      ]
